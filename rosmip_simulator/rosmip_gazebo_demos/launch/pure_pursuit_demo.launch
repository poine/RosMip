<?xml version="1.0"?>

<!--

Static map rosmip pure pursuit demonstration.
- rosmip in gazebo
- map server
- controllers
- rviz

-->

<launch>

  <arg name="world" default="empty"/>
  
  <arg name="world_path" value="$(find rosmip_worlds)/worlds/$(arg world).world" />
  <arg name="map_path"   default="$(find rosmip_worlds)/maps/track_ethz_dual.yaml" />

  <arg name="path_filename" default="$(find two_d_guidance)/paths/test/track_ethz_dual_01.npz" />
  <arg name="vel_setpoint" default="0.25" />

  <include file="$(find rosmip_gazebo)/launch/rosmip_world.launch" >
    <arg name="controller_enable_odom_tf" value="true" />
    <arg name="world_path" value="$(arg world_path)" />
  </include>


  <include file="$(find rosmip_navigation)/launch/demo_pure_pursuit.launch" >
    <arg name="map" value="$(arg map_path)" />
    <arg name="path_filename" value="$(arg path_filename)" />
    <arg name="vel_setpoint" value="$(arg vel_setpoint)" />
  </include> 
  


  <!--<node name="rviz" pkg="rviz" type="rviz" args="-d $(find rosmip_viz)/rviz/smocap.rviz" />-->

</launch>
